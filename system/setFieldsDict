FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha1 0
);

regions
(
    sphereToCell
    {
        centre (0 0 5.5e-3);
        radius 1e-3;
        fieldValues
        (
            volScalarFieldValue alpha1 1
            volVectorFieldValue U (0 0 -2)
        );
    }
);


